{
  "title": "Capacity Intel - Overview",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP p95 (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_duration_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "CPU avg",
      "targets": [
        {
          "expr": "avg(rate(container_cpu_usage_seconds_total[2m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Error rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m]))"
        }
      ]
    }
  ]
}